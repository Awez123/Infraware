{
  "planned_values": {
    "root_module": {
      "resources": [
        {
          "type": "aws_s3_bucket",
          "name": "public_bucket",
          "values": {
            "bucket": "advanced-public-bucket",
            "acl": "public-read"
          }
        },
        {
          "type": "aws_ebs_volume",
          "name": "unencrypted_ebs",
          "values": {
            "availability_zone": "us-east-1a",
            "size": 10,
            "encrypted": false
          }
        },
        {
          "type": "azurerm_storage_account",
          "name": "public_storage",
          "values": {
            "name": "advancestorageacct",
            "allow_blob_public_access": true
          }
        },
        {
          "type": "google_storage_bucket",
          "name": "public_gcs_bucket",
          "values": {
            "name": "advanced-public-gcs",
            "uniform_bucket_level_access": false
          }
        },
        {
          "type": "aws_security_group",
          "name": "open_sg",
          "values": {
            "name": "open-sg",
            "ingress": [{
              "from_port": 0,
              "to_port": 65535,
              "protocol": "tcp",
              "cidr_blocks": ["0.0.0.0/0"]
            }]
          }
        },
        {
          "type": "azurerm_network_security_group",
          "name": "open_nsg",
          "values": {
            "name": "open-nsg",
            "security_rule": [{
              "name": "AllowAll",
              "priority": 100,
              "direction": "Inbound",
              "access": "Allow",
              "protocol": "Tcp",
              "source_port_range": "*",
              "destination_port_range": "*",
              "source_address_prefix": "*",
              "destination_address_prefix": "*"
            }]
          }
        },
        {
          "type": "google_compute_firewall",
          "name": "open_firewall",
          "values": {
            "name": "open-firewall",
            "allow": [{
              "protocol": "tcp",
              "ports": ["0-65535"]
            }],
            "source_ranges": ["0.0.0.0/0"]
          }
        }
      ]
    }
  }
}
